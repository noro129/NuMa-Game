<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PlayGame"
    android:background="@color/background1">

    <LinearLayout
        android:id="@+id/columnsResults"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/matrixToFill"
        app:layout_constraintEnd_toEndOf="@+id/matrixToFill"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/matrixToFill">


    </LinearLayout>


    <LinearLayout
        android:id="@+id/rowsResults"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="@+id/matrixToFill"
        app:layout_constraintEnd_toStartOf="@+id/matrixToFill"
        app:layout_constraintTop_toTopOf="@+id/matrixToFill"
        app:layout_constraintVertical_bias="0.494">


    </LinearLayout>

    <LinearLayout
        android:id="@+id/matrixToFill"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

